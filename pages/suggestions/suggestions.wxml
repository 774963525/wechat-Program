<view class='nav' style='height:90px'>
  <!-- 返回图标 -->
  <view class='title_icon' style="font-size: 13px;position: absolute;top: 52px;left: 20px;color: #fff;" bindtap="back">
    <view class="cuIcon-back">返回</view>
  </view>
  <view class='title_text'>
    意见反馈
  </view>
</view>
<view class="container" style="margin-top:200px;">
	<view class="cu-form-group margin-top" style="height:200px; width:95%;margin: 0 auto;">
		<textarea  style="height:100px;" maxlength="-1" disabled="{{modalName!=null}}" bindinput="textareaAInput" placeholder="多行文本输入框"></textarea>
	</view>
	<view class="cu-form-group" style="width:95%;margin: 0 auto;">
		<view class="title">联系方式</view>
		<input placeholder="请输入您的联系方式" bindinput="contact"></input>
	</view>
</view>

<view class="padding flex flex-direction">
  <button class="cu-btn bg-grey lg" bindtap="back">取消</button>
  <button class="cu-btn bg-red margin-tb-sm lg" bindtap="submit">确认</button>
</view>